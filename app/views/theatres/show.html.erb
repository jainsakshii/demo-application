<h2><%= @theatre.name %> </h2>
<%= @theatre.contact_number %>
<%= @theatre.venue %>
<%= @theatre.theatre_administrator_id %>
<%= link_to "Edit", edit_theatre_path(@theatre), class: "btn btn-danger" %>
<%= link_to "Show Screen", theatre_screen_path(theatre_id: @theatre,id: @theatre.screen.id), class: "btn btn-danger" %>
<%= link_to "Destroy", theatre_path(@theatre), data: { method: :delete }, class: "btn btn-danger" %>
