<%= form_with url: movie_bookings_path(movie_id: @movie,show_id: @showtime,theatre_id:params[:theatre_id],screen_id:params[:screen_id]) do |form| %>
  <%= form.number_field :no_of_tickets, placeholder:"Enter Num. of seats to book" %>
  <%= form.submit "BOOK" %>
<% end %>


